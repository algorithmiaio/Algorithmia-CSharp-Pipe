<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.0;netcoreapp2.1;netcoreapp2.2</TargetFrameworks>
    <AssemblyVersion>1.1.1</AssemblyVersion>
    <FileVersion>1.1.1</FileVersion>
    <PackageVersion>1.1.1</PackageVersion>
    <InformationalVersion>1.1.1</InformationalVersion>
    <Authors>Algorithmia,JamesSutton</Authors>
    <title>.Net Pipe for Algorithmia</title>
    <Company>Algorithmia, Inc.</Company>
    <Description>An internal deployment package that allows for Algorithmia algorithm .net support</Description>
    <Copyright>Copyright © 2019 by Algorithmia Inc.</Copyright>
    <PackageLicenseUrl>https://github.com/algorithmiaio/algorithmia-c-sharp-dev/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/algorithmiaio/algorithmia-c-sharp-dev</PackageProjectUrl>
    <PackageIconUrl>https://cdn.algorithmia.com/versioned/images/logos/png/f4f0b308120f0feafe12581cf9f7cab9-bintreePurple.png</PackageIconUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/algorithmiaio/algorithmia-c-sharp-dev</RepositoryUrl>
    <PackageTags>Algorithmia, Algorithms, Machine Learning, Artificial Service, NLP, Sentiment Analysis, Microservices, Serverless</PackageTags>
    <RepositoryType>Git</RepositoryType>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageId>Algorithmia.Pipe</PackageId>
    <RootNamespace>pipe</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

</Project>

